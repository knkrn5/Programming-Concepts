                </mxCell>
                <mxCell id="195" value="3. known_host file: - this file let the client remember the host permanently without needing to comfirm the fingerprint of same host everytime" style="text;strokeColor=default;fillColor=none;align=left;verticalAlign=middle;spacingLeft=4;spacingRight=4;overflow=hidden;points=[[0,0.5],[1,0.5]];portConstraint=eastwest;rotatable=0;whiteSpace=wrap;html=1;dashed=1;dashPattern=1 4;" parent="190" vertex="1">
