                </mxCell>
                <mxCell id="194" value="2. Config file: - This is used to add list of all host with all its configurations like username, ip, private key path etc making easier for client to connect to host later" style="text;strokeColor=default;fillColor=none;align=left;verticalAlign=middle;spacingLeft=4;spacingRight=4;overflow=hidden;points=[[0,0.5],[1,0.5]];portConstraint=eastwest;rotatable=0;whiteSpace=wrap;html=1;dashed=1;dashPattern=1 4;" parent="190" vertex="1">
