                </mxCell>
                <mxCell id="261" value="4. authorized_keys file: - this file is used by server to store the public key of client, and then authenticate client without needing to enter passphrase every time" style="text;strokeColor=default;fillColor=none;align=left;verticalAlign=middle;spacingLeft=4;spacingRight=4;overflow=hidden;points=[[0,0.5],[1,0.5]];portConstraint=eastwest;rotatable=0;whiteSpace=wrap;html=1;dashed=1;dashPattern=1 4;" parent="190" vertex="1">
