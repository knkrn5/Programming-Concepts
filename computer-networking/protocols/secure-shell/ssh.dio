                    <mxGeometry y="170" width="288.75" height="50" as="geometry"/>
                </mxCell>
                <mxCell id="276" value="&lt;b&gt;Files transfering btw client/local and host/remote&lt;/b&gt;" style="swimlane;fontStyle=0;childLayout=stackLayout;horizontal=1;startSize=35;horizontalStack=0;resizeParent=1;resizeParentMax=0;resizeLast=0;collapsible=1;marginBottom=0;whiteSpace=wrap;html=1;rounded=1;fillColor=#fa6800;fontColor=#000000;strokeColor=#C73500;" parent="1" vertex="1">
                    <mxGeometry x="303.76" y="-370" width="267.5" height="225" as="geometry">
